{
  "name": "bachelor-project",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:host": "ng serve --host=0.0.0.0 --project=host --port=4200",
    "serve:mfe1": "ng serve --host=0.0.0.0 --project=mfe1 --port=4201",
    "serve:mfe2": "ng serve --host=0.0.0.0 --project=mfe2 --port=4202",
    "serve:mfe3": "ng serve --host=0.0.0.0 --project=mfe3 --port=4203",
    "serve:dist": "http-server dist -p 8080",
    "build:mfe1": "ng build mfe1 --prod --extra-webpack-config projects/mfe1/webpack.externals.js --single-bundle true --output-hashing none",
    "build:mfe2": "ng build mfe2 --prod --extra-webpack-config projects/mfe2/webpack.externals.js --single-bundle true --output-hashing none",
    "build:mfe3": "ng build mfe3 --prod --extra-webpack-config projects/mfe3/webpack.externals.js --single-bundle true --output-hashing none",
    "build:all": "ng build mfe1 --prod --extra-webpack-config projects/mfe1/webpack.externals.js --single-bundle true --output-hashing none && ng build mfe2 --prod --extra-webpack-config projects/mfe2/webpack.externals.js --single-bundle true --output-hashing none && ng build mfe3 --prod --extra-webpack-config projects/mfe3/webpack.externals.js --single-bundle true --output-hashing none"
  },
